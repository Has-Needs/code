{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": ["es2020"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "bundler",
    "outDir": "./dist"
  },
  "include": ["src"]
}